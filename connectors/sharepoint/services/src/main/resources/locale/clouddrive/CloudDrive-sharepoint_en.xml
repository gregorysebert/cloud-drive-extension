<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
  <UITabForm>
    <label>
      <connectTemplate>Connect SharePoint Server</connectTemplate>
    </label>
  </UITabForm>
  <!-- For menu caption in Action Bar -->
  <UIActionBar>
    <tooltip>
      <ConnectTemplate>Connect SharePoint Server</ConnectTemplate>
    </tooltip>
  </UIActionBar>
  <!-- For login form -->
  <CMISLoginController>
    <sharepoint>
      <title>Login Microsoft SharePoint server</title>
      <authHelp>Authenticate to select CMIS repository on your SharePoint server.</authHelp>
      <loginHelp>Select repository and complete login.</loginHelp>
    </sharepoint>
  </CMISLoginController>
</bundle>